{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "An application that allows user to publish insrtunctions on how to prepare a recipe",
  "main": "app.js",
  "scripts": {
    "migrate:dev": "sequelize db:migrate",
    "unmigrate:dev": "sequelize db:migrate:undo:all",
    "migrate:test": "sequelize db:migrate --env test",
    "unmigrate:test": "sequelize db:migrate:undo:all --env test",
    "seed": "sequelize db:seed:all",
    "unseed": "sequelize db:seed:undo:all",
    "start:dev": "nodemon --watch server --exec babel-node ./bin/www",
    "start:prod": "babel-node ./bin/www",
    "test": "SET NODE_ENV=test && mocha --compilers js:babel-core/register server/test/**.test/*.test.js --timeout 20000"
  },
  "author": "Kingsley Frank-Demesi",
  "license": "ISC",
  "devDepencies": {
    "babel-cli": "^6.24.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "nodemon": "^1.11.0",
    "sequelize-cli": "^2.8.0"
  },
  "dependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "eslint-config": "^0.3.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.2",
    "mocha": "^4.0.1",
    "morgan": "^1.8.2",
    "nodemailer": "^4.2.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.6",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "keypair": "^1.0.1",
    "nodemon": "^1.12.1",
    "sequelize-cli": "^3.0.0"
  },
